{
  "name": "yoastseo",
  "description": "Yoast client-side content analysis",
  "homepage": "https://github.com/Yoast/",
  "keywords": [
    "Yoast",
    "SEO",
    "readability",
    "inclusive language"
  ],
  "main": "build/index.js",
  "license": "GPL-3.0",
  "version": "1.91.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoast/wordpress-seo.git",
    "directory": "packages/yoastseo"
  },
  "files": [
    "build/**/*",
    "vendor",
    "images"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && yarn build:js",
